#include <windows.h>

IDI_ICON1		ICON		DISCARDABLE	"@APP_ICON_FILE@"

VS_VERSION_INFO VERSIONINFO
 FILEVERSION @TEMPLATE_VERSION_MAJOR@,@TEMPLATE_VERSION_MINOR@,@TEMPLATE_VERSION_PATCH@,0
 PRODUCTVERSION @TEMPLATE_VERSION_MAJOR@,@TEMPLATE_VERSION_MINOR@,@TEMPLATE_VERSION_PATCH@,0
 FILEFLAGS 0x0L
 FILEFLAGSMASK 0x3fL
 FILEOS 0x00040004L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "000004b0"
        BEGIN
            VALUE "CompanyName", "@TEMPLATE_ORGANIZATION@"
            VALUE "InternalName", "@PROJECT_NAME@"
            VALUE "OriginalFilename", "@PROJECT_NAME@.exe"
            VALUE "ProductName", "@PROJECT_NAME@"
        END
    END
END
